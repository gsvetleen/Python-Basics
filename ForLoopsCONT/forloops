number = "9, 223,372,036,854,775,807"
cleanedNumber =''

# assign each variable without indexing
for char in number:
    if char in '0123456789':
        cleanedNumber = cleanedNumber + char

newNumber = int(cleanedNumber)
print("The number is {} ".format(newNumber))


# includes arrays
for state in ["not pinin'","no more", "a stiff","berefit of life"]:
    print("This parrot is " + state)
    # print("This parrot is {}".format(state))

# for loop with steps to end range being in 5
for i in range(0,100,5):
    print("i is {} ".format(i))

# nested loop example

for i in range(1,13):
    for j in range(1,13):
        print("{} times {} is {}".format(i,j,i*j))
    print("=====================")

# same example but with tab instead of new line
for i in range(1,13):
    for j in range(1,13):
        print("{} times {} is {}".format(i,j,i*j),end='\t')
    print("=====================")